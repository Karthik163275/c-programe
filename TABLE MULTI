#include<stdio.h>
int main( ){
	int m,n ,i;
	printf("enter the number you want to print the multiplication table of:");
	scanf("%d",&m);
	printf("enter the num up to which you want to print the multipliication table:");
	scanf("%d",&n);
	for(i=1;i<=n;i++){
	printf("%d*%d=%d\n",m,i,m*i);
}
return 0;
}
