// Working of arithmetic operators
#include <stdio.h>
int main()
{
    int a ,b , c;
    printf("enter the first digit: ");
    scanf("%d",&a);
    printf("enter the second digit: ");
    scanf("%d",&b);
    c = a+b;
    printf("a+b = %d \n",c);
    c = a-b;
    printf("a-b = %d \n",c);
    c = a*b;
    printf("a*b = %d \n",c);
    c = a/b;
    printf("a/b = %d \n",c);
    c = a%b;
    printf("Remainder when a divided by b = %d \n",c);    
    return 0;
}
