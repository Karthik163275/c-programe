#include<stdio.h>
int main()
{
	int a[100],n,i,max,min;
	printf("Enter the size of array :");
	scanf("%d",&n);
	printf("Enter the array elements :");
	for(i=0;i<n;i++)
	{
		scanf("%d",&a[i]);
	}
	min=max=a[0];
	for(i=0;i<n;i++)
	{
		if(min>a[i])
		min=a[i];
		if(max<a[i])
		max=a[i];
	}
	printf("min of arrays:%d \n",min);
	printf("max of arrays:%d \n",max);
	return 0;
}
