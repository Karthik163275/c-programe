#include<stdio.h>
int main( )
{
	int i,j,rows;
	//variable declaration
	printf("enter the number of rows:\n ");
	scanf("%d",&rows);
	printf("\n");
	//takes input from user 
	for(i=1;i<=rows;i++)
	{
		for(j=i;j>=1;j--){
			printf("%d",j);
			//display pattern
		}
		printf("\n");
		//move to next line
	}
	return 0;
}
